      "microsoft/resnet-50",
      "facebook/convnext-tiny-224"
    ],
    "audio": [
      "openai/whisper-tiny",
      "facebook/wav2vec2-base"
    ],
    "multimodal": [
      "openai/clip-vit-base-patch32"
